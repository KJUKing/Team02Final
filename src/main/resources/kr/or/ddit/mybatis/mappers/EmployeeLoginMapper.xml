<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="kr.or.ddit.employee.dao.EmployeeLoginMapper">

	<select id="loginResult" resultType="EmployeeVO">
		SELECT * FROM EMPLOYEE
		WHERE EMP_ID = #{empId} AND EMP_PASS = #{empPass}
	</select>

</mapper>